{
  "name": "nwb-electron-starter",
  "version": "1.0.0",
  "description": "Describe nwb-electron-starter here",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "nwb build && babel electron/ -o dist/index.js && npm run package",
    "watch-electron-main": "babel main -d dist -w",
    "clean": "nwb clean",
    "start": "node dev-server.js --run-electron",
    "test": "nwb test",
    "test:watch": "nwb test --server",
    "package": "rm -rf ./release && electron-packager . Whatsgit --overwrite --asar='true' --ignore='renderer|electron|node_modules|tests' --prune --platform=darwin --arch=x64 --version=1.3.2 --out='release'"
  },
  "dependencies": {
    "electron-window-state": "^3.0.3",
    "flex-component": "^0.1.3",
    "react": "^15.3.0",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-actions": "^0.9.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "component-resolver-webpack": "^0.4.0",
    "electron-prebuilt": "^1.3.2",
    "nwb": "^0.9.2",
    "nwb-sass": "^0.5.0",
    "webpack": "^1.13.0"
  },
  "author": "",
  "license": "MIT",
  "repository": ""
}
